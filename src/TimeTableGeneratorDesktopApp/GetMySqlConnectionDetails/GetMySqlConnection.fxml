<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TimeTableGeneratorDesktopApp.GetMySqlConnectionDetails.GetMySqlConnectionController">
   <children>
      <Label layoutX="38.0" layoutY="34.0" text="Enter MySQL Connection Details">
         <font>
            <Font name="Segoe UI" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="txtPortNo" layoutX="38.0" layoutY="99.0" promptText="Enter Port No" />
      <TextField fx:id="txtUser" layoutX="38.0" layoutY="156.0" promptText="Enter User" />
      <Button fx:id="btnSet" layoutX="38.0" layoutY="271.0" mnemonicParsing="false" onAction="#setOnActionSetConnectionSettings" text="SET">
         <font>
            <Font name="Segoe UI" size="13.0" />
         </font>
      </Button>
      <Button fx:id="btnDefaults" layoutX="435.0" layoutY="38.0" mnemonicParsing="false" onAction="#setOnActionSetDefaults" text="SET DEFAULT SETTINGS" />
      <PasswordField fx:id="txtPassword" layoutX="38.0" layoutY="213.0" promptText="Enter root password" />
   </children>
</AnchorPane>
