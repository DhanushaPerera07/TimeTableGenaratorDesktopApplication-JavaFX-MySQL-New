<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TimeTableGeneratorDesktopApp.Tags.TagsController">
   <children>
      <AnchorPane fx:id="mainPane" prefHeight="600.0" prefWidth="800.0">
         <children>
            <Label layoutX="30.0" layoutY="20.0" text="Tags">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Pane layoutX="209.0" layoutY="94.0">
               <children>
                  <ListView fx:id="tagsList3" layoutX="265.0" onMouseClicked="#deleteTags" prefHeight="200.0" prefWidth="94.0">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></ListView>
                  <ListView fx:id="tagsList2" layoutX="138.0" prefHeight="200.0" prefWidth="94.0" />
                  <Button fx:id="tagSubmit" layoutX="158.0" layoutY="235.0" mnemonicParsing="false" onAction="#submitTags" onMouseClicked="#submitTags" text="Submit">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
                  <ListView fx:id="tagsList1" onMouseClicked="#setTagsList2" prefHeight="200.0" prefWidth="94.0">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></ListView>
               </children>
            </Pane>
            <Label layoutX="225.0" layoutY="77.0" text="System List" />
            <Label layoutX="359.0" layoutY="76.0" text="Selected Tags" />
            <Label layoutX="495.0" layoutY="76.0" text="Picked List" />
            <Label layoutX="117.0" layoutY="356.0" text="Instructions">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="659.0" layoutY="545.0" onMouseClicked="#ActionEventManageTags" text="Manage Picking List" underline="true">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Label>
            <GridPane layoutX="178.0" layoutY="379.0" prefHeight="150.0" prefWidth="450.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="1. Select the needed tags for generating time tables from the System List." />
                  <Label text="2. Then those selected tags will be shown in the Selected tags box." GridPane.rowIndex="1" />
                  <Label text="3. After selecting the wanted tags, then click the sbumit button " GridPane.rowIndex="2" />
                  <Label text="4. The selected tags will be move to the Picked tags box. " GridPane.rowIndex="3" />
                  <Label prefHeight="17.0" prefWidth="434.0" text="5. You can remove a tag by clicking the particular tag in the savPicked list" GridPane.rowIndex="4" />
                  <Label prefHeight="17.0" prefWidth="434.0" text="You can use use picking list function to add very new tags in to the systsem." GridPane.rowIndex="5" />
               </children>
            </GridPane>
         </children>
      </AnchorPane>
   </children>
</Pane>
